{
  "name": "OctoLinker",
  "version": "6.3.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn lint",
    "test": "jest",
    "e2e": "node ./e2e/generate-fixtures-json.js && jest --config=e2e/jest-e2e.config.js",
    "test:watch": "jest --watch",
    "version": "dot-json assets/manifest.json version $npm_package_version && dot-json packages/core/package.json version $npm_package_version && git add assets/manifest.json packages/core/package.json",
    "release:cws": "cd dist && webstore upload --auto-publish",
    "release:amo": "cd dist && web-ext-submit",
    "release": "yarn release:cws && yarn release:amo",
    "build": "webpack --mode=production",
    "start": "webpack --watch --mode=development",
    "chrome-open": "yarn build && yarn chrome-launch --",
    "chrome-launch": "node scripts/chrome-launch.js",
    "firefox-open": "yarn build && yarn firefox-launch --",
    "firefox-launch": "web-ext run --source-dir dist --pref startup.homepage_welcome_url=https://github.com/OctoLinker/OctoLinker/blob/master/package.json"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "async": "^3.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-jest": "^25.3.0",
    "chrome-launch": "^1.1.4",
    "chrome-webstore-upload-cli": "^1.2.0",
    "codecov": "^3.5.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "dot-json": "^1.2.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^25.1.0",
    "jest-fetch-mock": "^3.0.2",
    "jest-puppeteer": "^4.2.0",
    "json-loader": "^0.5.7",
    "prettier": "^2.0.4",
    "puppeteer": "^2.0.0",
    "recursive-readdir": "^2.2.2",
    "sinon": "^9.0.0",
    "style-loader": "^1.0.0",
    "web-ext": "^4.0.0",
    "web-ext-submit": "^4.1.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.4",
    "webpack-extension-reloader": "^1.1.4"
  },
  "optionalDependencies": {
    "fsevents": "2.1.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "dependencies": {}
}
