{
  "name": "OctoLinker",
  "version": "0.0.0",
  "engines": {
    "node": ">= 16",
    "npm": ">= 7"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn lint",
    "test": "jest",
    "e2e": "node ./e2e/generate-fixtures-json.js && jest --config=e2e/jest-e2e.config.js",
    "test:watch": "jest --watch",
    "release:cws": "cd dist && npx chrome-webstore-upload-cli@next upload --auto-publish",
    "release:amo": "cd dist && npx web-ext-submit@5",
    "release": "yarn release:cws && yarn release:amo",
    "build": "webpack --mode=production",
    "start": "webpack --watch --mode=development",
    "chrome-open": "yarn build && yarn chrome-launch --",
    "chrome-launch": "node scripts/chrome-launch.js",
    "firefox-open": "yarn build && yarn firefox-launch --",
    "firefox-launch": "web-ext run --source-dir dist --pref startup.homepage_welcome_url=https://github.com/OctoLinker/OctoLinker/blob/main/package.json",
    "safari-open": "yarn build && yarn safari-pack && yarn safari-launch --",
    "safari-pack": "xcodebuild -project 'safari/OctoLinker.xcodeproj'",
    "safari-launch": "open 'safari/build/Release/OctoLinker.app'"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.21.4",
    "assert": "^2.0.0",
    "async": "^3.2.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-preset-jest": "^29.4.3",
    "chrome-launch": "^1.1.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "dot-json": "^1.2.2",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-puppeteer": "^6.2.0",
    "json-loader": "^0.5.7",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.7",
    "process": "^0.11.10",
    "puppeteer": "^19.11.0",
    "recursive-readdir": "^2.2.3",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.2",
    "url": "^0.11.0",
    "vm-browserify": "^1.1.2",
    "web-ext": "^7.6.0",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-extension-reloader": "^1.1.4"
  },
  "optionalDependencies": {
    "fsevents": "2.3.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "dependencies": {}
}
