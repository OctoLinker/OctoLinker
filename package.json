{
  "name": "OctoLinker",
  "version": "4.1.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "lib/app.js",
  "scripts": {
    "postinstall": "ln -sf ./scripts/hooks/pre-commit .git/hooks/pre-commit",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "karma start",
    "test:watch": "karma start --no-single-run --auto-watch",
    "chrome-manifest": "mkdir -p dist && json -e 'delete this.applications; this.permissions.shift()' < assets/manifest.json > dist/manifest.json",
    "chrome-build": "npm run chrome-manifest && webpack",
    "chrome-dist": "npm run chrome-build -- -p",
    "chrome-watch": "npm run chrome-manifest && webpack --watch",
    "chrome-pack": "npm run chrome-dist && mkdir -p out && zip --recurse-paths out/chrome-octolinker-$npm_package_version.zip dist/ --exclude dist/ \\* out/\\* node_modules/\\*",
    "firefox-manifest": "mkdir -p dist && cp assets/manifest.json dist/manifest.json",
    "firefox-build": "npm run firefox-manifest && webpack",
    "firefox-dist": "npm run firefox-build -- -p",
    "firefox-watch": "npm run firefox-manifest && webpack --watch",
    "firefox-pack": "npm run firefox-dist && mkdir -p out && zip --recurse-paths out/firefox-octolinker-$npm_package_version.zip dist/ --exclude dist/ \\* out/\\* node_modules/\\*",
    "firefox-open": "npm run firefox-build && web-ext run --source-dir dist",
    "pack-repo": "mkdir -p out && zip --recurse-paths out/octolinker-repository-$npm_package_version.zip * --exclude dist/\\* out/\\* node_modules/\\*"
  },
  "dependencies": {
    "JSONPath": "^0.11.2",
    "babel-polyfill": "^6.3.14",
    "builtins": "^1.0.3",
    "escape-regex-string": "^1.0.4",
    "findandreplacedomtext": "^0.4.4",
    "github-injection": "^0.2.0",
    "github-url-from-git": "^1.4.0",
    "github-url-from-username-repo": "^1.0.2",
    "jquery": "^2.1.4",
    "semver": "^5.1.0",
    "semver-regex": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "copy-webpack-plugin": "^3.0.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "2.1.1",
    "json": "^9.0.4",
    "json-loader": "^0.5.4",
    "karma-fixture": "^0.2.5",
    "karma-html2js-preprocessor": "^0.1.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.18",
    "sinon": "^2.0.0-pre",
    "web-ext": "^1.0.1",
    "webpack": "^1.13.1",
    "yargs": "^4.7.1"
  },
  "private": true
}
