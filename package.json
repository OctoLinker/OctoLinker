{
  "name": "OctoLinker",
  "version": "6.8.1",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn lint",
    "test": "jest",
    "e2e": "node ./e2e/generate-fixtures-json.js && jest --config=e2e/jest-e2e.config.js",
    "test:watch": "jest --watch",
    "version": "./scripts/version.sh",
    "release:cws": "cd dist && webstore upload --auto-publish",
    "release:amo": "cd dist && web-ext-submit",
    "release": "yarn release:cws && yarn release:amo",
    "build": "webpack --mode=production",
    "start": "webpack --watch --mode=development",
    "chrome-open": "yarn build && yarn chrome-launch --",
    "chrome-launch": "node scripts/chrome-launch.js",
    "firefox-open": "yarn build && yarn firefox-launch --",
    "firefox-launch": "web-ext run --source-dir dist --pref startup.homepage_welcome_url=https://github.com/OctoLinker/OctoLinker/blob/main/package.json",
    "safari-open": "yarn build && yarn safari-pack && yarn safari-launch --",
    "safari-pack": "xcodebuild -project 'safari/OctoLinker.xcodeproj'",
    "safari-launch": "open 'safari/build/Release/OctoLinker.app'"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-react-jsx": "^7.12.16",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "async": "^3.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-jest": "^26.1.0",
    "chrome-launch": "^1.1.4",
    "chrome-webstore-upload-cli": "^1.2.1",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.0.2",
    "dot-json": "^1.2.1",
    "dotenv": "^8.0.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.2",
    "jest-puppeteer": "^4.2.0",
    "json-loader": "^0.5.7",
    "prettier": "^2.2.1",
    "puppeteer": "^2.0.0",
    "recursive-readdir": "^2.2.2",
    "sinon": "^9.2.4",
    "style-loader": "^2.0.0",
    "web-ext": "^5.4.1",
    "web-ext-submit": "^5.5.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0",
    "webpack-extension-reloader": "^1.1.4"
  },
  "optionalDependencies": {
    "fsevents": "2.2.1"
  },
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "dependencies": {}
}
